-- Webhook para onde a notificação será enviada
local webhook_url = "https://discord.com/api/webhooks/1406982165257785515/f-Fhp7M0hDM3-doX8wWN7MR8MuPgj5mzx5JeJdDO4y-3KAkBlrNAgR6PdcWWEtbL8t4v"

-- Dados da notificação (pode personalizar)
local data = {
    ["content"] = nil,
    ["embeds"] = {{
        ["title"] = "Script executado!",
        ["description"] = "Um usuário acabou de executar o script.",
        ["type"] = "rich",
        ["color"] = tonumber(0x00ff00),
        ["fields"] = {
            {
                ["name"] = "Hora",
                ["value"] = os.date("%d/%m/%Y %H:%M:%S"),
                ["inline"] = true
            },
            {
                ["name"] = "Executor",
                ["value"] = (identifyexecutor and identifyexecutor()) or "Desconhecido",
                ["inline"] = true
            }
        }
    }}
}

-- Codifica os dados em JSON
local HttpService = game:GetService("HttpService")
local json_data = HttpService:JSONEncode(data)

-- Envia o POST para o webhook
local success, err = pcall(function()
    request = (syn and syn.request) or (http and http.request) or (http_request) or (fluxus and fluxus.request) or (krnl and krnl.httpRequest)
    if request then
        request({
            Url = webhook_url,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = json_data
        })
    else
        warn("Executor não suportado para envio de webhooks.")
    end
end)

if not success then
    warn("Erro ao enviar webhook:", err)
end
